<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This permission set enables users to create and edit internal users</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Create Internal Users</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>ManageInternalUsers</name>
    </userPermissions>
</PermissionSet>