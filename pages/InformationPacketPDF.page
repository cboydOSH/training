							<th/>
							<th>
								<apex:outputText rendered="{!IF(CONTAINS(programName,'Basic'),true,false)}">BASIC</apex:outputText>
								<apex:outputText rendered="{!IF(CONTAINS(programName,'Enhanced'),true,false)}">ENHANCED</apex:outputText>
								<apex:outputText rendered="{!IF(CONTAINS(programName,'Crown'),true,false)}">CROWN</apex:outputText>
								<apex:outputText value=" In Network" />
							</th>
							<th>
								<apex:outputText rendered="{!IF(CONTAINS(programName,'Basic'),true,false)}">BASIC</apex:outputText>
								<apex:outputText rendered="{!IF(CONTAINS(programName,'Enhanced'),true,false)}">ENHANCED</apex:outputText>
