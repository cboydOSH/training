							<td style="text-align:left;">ISA</td>
							<td style="text-align:center;" colspan="2">{!productsComplete[prod]['ISAAMT']}</td>
						</tr>
						<apex:repeat value="{!completedKeysOrder}" var="key">
							<tr>
								<apex:variable var="key2" value="{!key}2" />
								<td style="text-align:left;">{!mapCodesWithLabels[key]}</td>
