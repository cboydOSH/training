    <fields>
        <fullName>Age__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISBLANK(Completed_Date__c)), Completed_Date__c  - CreatedDate,NOW() - CreatedDate)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age</label>
