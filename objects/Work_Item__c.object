    <validationRules>
        <fullName>Cannot_edit_a_completed_Work_Item</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 Record_Locked__c = TRUE,
 Completed_Date__c &gt; NOW() + 0.002
)</errorConditionFormula>
        <errorMessage>You cannot edit a Work Item that has been completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_owner_can_edit_work_item</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 $Profile.Name &lt;&gt; &quot;System Administrator&quot;,
 $User.Id &lt;&gt;  OwnerId,
 NOT(ISCHANGED(OwnerId))
)</errorConditionFormula>
        <errorMessage>You must own this Work Item to edit or make changes.</errorMessage>
    </validationRules>
