    <validationRules>
        <fullName>Manager_field_req_during_user_creation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISNEW(),
ISCHANGED( ManagerId )
),
ISBLANK( ManagerId ),
NOT(CONTAINS(Profile.Name, &quot;Community&quot; ))  
)</errorConditionFormula>
        <errorDisplayField>ManagerId</errorDisplayField>
        <errorMessage>Manager field has to be filled before creating users</errorMessage>
    </validationRules>
